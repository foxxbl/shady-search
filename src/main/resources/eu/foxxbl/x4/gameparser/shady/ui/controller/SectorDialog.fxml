<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="dialog" alignment="CENTER" prefHeight="450.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="eu.foxxbl.x4.gameparser.shady.ui.controller.SectorDialog">
  <Label fx:id="sectorName" alignment="CENTER" textAlignment="CENTER" prefWidth="1000.0">
    <font>
      <Font name="System Bold" size="16.0"/>
    </font>
  </Label>
  <TableView fx:id="blackMarketeerTable" prefHeight="400.0" prefWidth="1200.0" VBox.vgrow="ALWAYS">
    <columns>
      <TableColumn fx:id="stationCode" prefWidth="100.0" text="StationCode"/>
      <TableColumn fx:id="stationMacro" prefWidth="400.0" text="StationMacro"/>
      <TableColumn fx:id="blackMarketeerName" prefWidth="400.0" text="Black Marketeer Name"/>
      <TableColumn fx:id="status" prefWidth="150.0" text="Status"/>
      <TableColumn fx:id="voiceLeaks" prefWidth="150.0" text="VoiceLeaks"/>
    </columns>
  </TableView>
  <Button fx:id="closeButton" mnemonicParsing="false" text="Close"/>

</VBox>
